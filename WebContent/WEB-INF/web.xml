<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://hahahaha/" xmlns:web="http://hahahahahaha/" xmlns:web_1="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://hahahahaha/ hohohohohoho http://java.sun.com/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee" id="WebApp_ID" version="3.0">
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>mvc-dispatcher</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>
      <description>加载/WEB-INF/beans/目录下的所有XML作为Spring MVC的配置文件</description>
      <param-name>contextConfigLocation</param-name>
      <param-value>/WEB-INF/beans/*.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>mvc-dispatcher</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
</web-app>